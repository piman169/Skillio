<div>
  <div>
    <div>ログイン</div>
    <div>
      <div>
        <% if @error_message %>
          <div>
            <%= @error_message %>
          </div>
        <% end %>

        <%= form_tag("/login") do %>
        <div class="post-container">
        <div>
          <h4>メールアドレス</h4>
          <input name="email" value="<%= @email %>">
          <h4>パスワード</h4>
          <input type="password" name="password" value="<%= @password %>">
          <br>
          <input type="submit" value="ログイン">
        </div>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
